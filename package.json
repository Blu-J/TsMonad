{
  "name": "tsmonad",
  "version": "0.8.0",
  "description": "TsMonad - fun-size monads library for TypeScript",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE-MIT",
    "README.md"
  ],
  "directories": {},
  "scripts": {
    "clean": "rm -rf dist lib",
    "prebuild": "npm run test",
    "build": "parcel build src/index.ts -d dist && cp ./lib/src/**.d.ts ./dist",
    "pretest": "npm run clean && tsc",
    "prepublish": "npm run build",
    "test": "mocha lib/test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cbowdon/TsMonad.git"
  },
  "keywords": [
    "typescript",
    "monad",
    "functional"
  ],
  "author": {
    "name": "Chris Bowdon",
    "url": "https://github.com/cbowdon"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/underscore": "^1.8.0",
    "assert": "^1.4.1",
    "mocha": "^2.5.3",
    "parcel-bundler": "^1.6.2",
    "typescript": "2.7.2",
    "underscore": "^1.8.3"
  }
}
